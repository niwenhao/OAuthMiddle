<%= form_for([owner, token, auth_property]) do |f| %>
  <% if auth_property.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(auth_property.errors.count, "error") %> prohibited this auth_property from being saved:</h2>

      <ul>
      <% auth_property.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :key %>
    <%= f.text_field :key %>
  </div>

  <div class="field">
    <%= f.label :txtValue %>
    <%= f.text_area :txtValue %>
  </div>

  <div class="field">
    <%= f.label :numValue %>
    <%= f.text_field :numValue %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
